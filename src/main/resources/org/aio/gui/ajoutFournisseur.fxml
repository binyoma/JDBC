<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.aio.gui.AjoutFournisseurController">
   <children>
      <Label layoutX="157.0" layoutY="20.0" text="Ajout d'un fournisseur">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <Label layoutX="70.0" layoutY="102.0" text="Nom" />
      <TextField fx:id="nom" layoutX="165.0" layoutY="98.0" onKeyReleased="#checkNom" prefHeight="26.0" prefWidth="335.0" />
      <Label layoutX="70.0" layoutY="148.0" text="Rue" />
      <TextField fx:id="rue" layoutX="169.0" layoutY="144.0" onKeyReleased="#checkRue" prefHeight="26.0" prefWidth="327.0" />
      <Label layoutX="70.0" layoutY="194.0" text="CP/Ville" />
      <TextField fx:id="cp" layoutX="169.0" layoutY="194.0" onKeyReleased="#checkCP" prefHeight="26.0" prefWidth="85.0" />
      <TextField fx:id="ville" layoutX="275.0" layoutY="194.0" onKeyReleased="#checkVille" prefHeight="26.0" prefWidth="225.0" />
      <Label layoutX="70.0" layoutY="240.0" text="contact" />
      <TextField fx:id="contact" layoutX="169.0" layoutY="236.0" onKeyReleased="#checkContact" prefHeight="26.0" prefWidth="327.0" />
      <Button fx:id="ajout" layoutX="304.0" layoutY="324.0" mnemonicParsing="false" onAction="#ajouter" text="Ajouter" />
      <Button fx:id="annulation" layoutX="443.0" layoutY="324.0" mnemonicParsing="false" onAction="#annuler" text="annuler" />
   </children>
</AnchorPane>
